    "semver": "^5.3.0",
    "trash": "^4.0.1",
    "uglify-js": "mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "yargs": "^8.0.2"
  },
  "optionalDevDependencies": {
